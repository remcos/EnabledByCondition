<?xml version="1.0" encoding="utf-8" ?>
<widget id="EnabledByCondition.widget.EnabledByCondition" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>EnabledByCondition</name>
    <description>The description of this widget.</description>

    <icon>
    
    </icon>

    <properties>
        <property key="buttonClassName" type="string" required="false">
            <caption>Button class name</caption>
            <category>Behaviour</category>
            <description>The class of the button that will be enabled</description>
        </property>
        <property key="microflowName" type="microflow" required="false">
            <caption>Boolean Microflow</caption>
            <category>Data source</category>
            <description>Select the Microflow that determines whether the button should be enabled.</description>
            <returnType type="Boolean" />
        </property>
        <property key="nanoflow" type="nanoflow" required="false">
            <caption>Boolean Nanoflow</caption>
            <category>Data source</category>
            <description>Or... Select the Nanoflow that determines whether the button should be enabled.</description>
            <returnType type="Boolean" />
        </property>
    </properties>
</widget>
